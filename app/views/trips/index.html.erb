<div class="container">
<h2> All trips</h2>

<div id="map"
    style="width: 345px; height: 400px; position: sticky; top: 0 !important; right: auto !important; left: auto !important;"
    data-markers="<%= @markers.to_json %>"
    data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
  </div>




<br>
<% @trips.each do |trip| %>
    <%= link_to trip_path(trip) do %>
    <h3> <strong><%= trip.name  %></strong></h3>
    <% end %>
    <% end %>



</div>
